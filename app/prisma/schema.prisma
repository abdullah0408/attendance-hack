generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id       String  @id @default(cuid())
  clerkId  String  @unique
  email    String  @unique
  password String
  userData Json?   @default("{}")
  inUse    Boolean @default(false)
  stuID    String  @unique
  cookie   String?

  createdAt DateTime @default(now())
}
